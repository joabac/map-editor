crear tablas con postgis

Select AddGeometryColumn ('public','puntos','the_geom',22185,'POINT',2);

Select AddGeometryColumn ('public','poligonos','the_geom',22185,'POLYGON',2);

Select AddGeometryColumn ('public','segmentos','the_geom',22185,'LINESTRING',2);



